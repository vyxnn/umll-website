{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "date": {
      "type": "datetime",
      "required": true
    },
    "summary": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "eventUrl": {
      "type": "string",
      "default": "https://www.facebook.com/UniMelbLoveLettersSociety"
    }
  }
}
